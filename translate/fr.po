#. type: Title =
#: ../en/modules/charts/pages/charts.adoc:4
#, no-wrap
msgid "Installing a Chart"
msgstr "Installer un graphique"

#. type: Plain text
#: ../en/modules/charts/pages/charts.adoc:7
msgid ""
"For demonstration a freely available U.S. chart (CAPE SABLE TO CAPE "
"HATTERAS) will be used."
msgstr ""
"Pour la démonstration, une carte américaine disponible gratuitement (CAPE "
"\"\n"
"\"SABLE TO CAPE HATTERAS) sera utilisée."

#. type: Title ==
#: ../en/modules/charts/pages/charts.adoc:8
#, no-wrap
msgid "Downloading and installing a U.S. chart"
msgstr "Téléchargement et installation d'une carte des États-Unis"

#. type: Plain text
#: ../en/modules/charts/pages/charts.adoc:12
msgid "btn:[Options] image:2.0.jpg[] on the tool bar btn:[Charts]"
msgstr "btn:[Options] image:2.0.jpg[] sur la barre d'outils btn:[Graphiques]"

#. type: Plain text
#: ../en/modules/charts/pages/charts.adoc:14
msgid "image:2.1.jpg[]"
msgstr "image:2.1.jpg[]image:2.1.jpg[]"

#. type: Plain text
#: ../en/modules/charts/pages/charts.adoc:16
msgid "Select the __Chart Downloader__ tab"
msgstr "Sélectionnez l'onglet __Chart Downloader__"

#. type: Plain text
#: ../en/modules/charts/pages/charts.adoc:18
msgid "image:2.2.jpg[]"
msgstr "image:2.2.jpg[]"

#. type: Plain text
#: ../en/modules/charts/pages/charts.adoc:20
msgid "Press btn:[Add Catalog]"
msgstr "Appuyez sur btn : [Ajouter un catalogue]"

